#include<bits/stdc++.h>
#define int long long 
using namespace std;
signed main(){
    int n,d;
    cin>>n>>d;
    int a[n];
    int sum=0;
    for(int i=0;i<n;i++){
        cin>>a[i];
        sum+=a[i];
    }
   // cout<<sum<<endl;
    if(((n-1)*10 +sum)>d){
        cout<<"-1\n";
        return 0;
    }
    cout<<2*(n-1)+(d-((n-1)*10+sum))/5<<endl;   // remaining time will be for jokes and between the every rest of 10 minutes churu make 2 jokes of 5 minutes
    return 0;
}
