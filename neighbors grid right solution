#include<iostream>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n,m;
        cin>>n>>m;
        int flag=0;
        int a[n][m],b[n][m],i,j;
        for(i=0;i<n;i++)
        {
            for(j=0;j<m;j++)
            {
                cin>>a[i][j];
                if((i==0 && j==0) || (i==0 && j==(m-1)) || (i==(n-1) && j==0) || (i==(n-1) && j==(m-1)))
                {
                    b[i][j]=2;
                }
                else if((i!=0) && (j!=0) && (i!=(n-1)) && (j!=(m-1)))
                {
                    b[i][j]=4;
                }
                else
                {
                    b[i][j]=3;
                }
                if(a[i][j] > b[i][j])
                {
                    flag=1;
                    //break;  if we use break here then it will break fort loop for all test case .means suppose in second case for loop goes break; then this for loop will not consider for test cases after this 
                    //for loop only work for case 1 and 2 but after 2 it will not work because it will goes break;in second case
                }
            }
        }
        if(flag==1)
        {
            cout<<"NO"<<"\n";
        }
        else
        {
            cout<<"YES\n";
            for(i=0;i<n;i++)
            {
                for(j=0;j<m;j++)
                {
                    cout<<b[i][j]<<" ";
                }
                cout<<"\n";
            }
        }
    }
    return 0;
}
